{
  "template": "etlRealtime",
  "config": {
    "source": {
      "name": "Kafka",
      "properties": {
        "kafka.partitions": 10,
        "kafka.topic": "metrics",
        "kafka.zookeeper": "localhost:2181/cdap/kafka"
      }
    },
    "sink": {
      "name": "Stream",
      "properties": {
        "name": "metrics",
        "body.field": "message"
      }
    },
    "transforms": []
  }
}
